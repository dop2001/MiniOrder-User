page {
  background-color:#fff;
}
.container-img {
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  /* background-color: blue; */
}
.img {
  display: flex;
  width: 120rpx;
  height: 120rpx;
}
.container-success {
  width: 100%;
  height: 300rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  /* background-color: red; */
}
.money {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 350rpx;
  height: 90rpx;
  font-size: xx-large;
  font-weight: 600;
  /* background-color: rosybrown; */
}
.name {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
  font-size: medium;
  font-weight: 600;
  color: gray;
  margin-top: 10rpx;
  /* background-color: green; */
}

.container-button {
  position: fixed;
  bottom: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 110rpx;
  /* background-color: #D52E35; */
}
.button {
  display: flex;
  width: 310rpx;
  height: 70rpx;
  align-items: center;
  justify-content: center;
  border-radius: 100rpx;
  background-color: #F52E35; 
}